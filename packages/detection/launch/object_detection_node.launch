<?xml version="1.0"?>
<launch>

  <arg name="veh" doc="Name of vehicle. ex: megaman"/>

  <group ns="$(arg veh)">  

    <node pkg="detection" type="detector_node.py" name="detector_node" output="screen">
        <rosparam command="load" file="$(find detection)/config/object_detection_node/default.yaml"/>

    </node>

  </group>

</launch>
